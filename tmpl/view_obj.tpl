<div class="row">
    <div class="col-md-12 title_view">
        %title_obj%
    </div>
</div>
<div class="row">
    <div class="col-md-12 time_view">
        %time%
    </div>
</div>
<div class="row">
    <div class="col-md-12 demo-gallery">
        <ul id="lightgallery">
            %gallery%
        </ul>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="block_content block_obj_title">
            <div class="row">
                <div class="col-md-11 col-md-offset-1 margin_top_">
                    <span class="block_obj_desc_title">Цена</span>
                    %price%
                </div>
            </div>
            <div class="row">
                <div class="col-md-11 col-md-offset-1 margin_top_">
                    <span class="block_obj_desc_title">Город</span>
                    %city%
                </div>
            </div>
            <div class="row">
                <div class="col-md-11 col-md-offset-1 margin_top_">
                    <span class="block_obj_desc_title">Адрес</span>
                    %add%
                </div>
            </div>
            <div class="row">
                <div class="col-md-11 col-md-offset-1 margin_top_">
                    <span class="block_obj_desc_title">Контакты</span>
                    %contact%
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-3">
        <div class="block_content block_obj_title">
            <div class="row">
                <div class="col-md-12">
                    <div class="block_obj_comfort_title">Удобства</div>
                    <div class="block_obj_comfort">
                        %comforts%
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-9 ">
        <div class="block_content block_obj_title">
            <div class="row">
                <div class="col-md-11 col-md-offset-1 margin_top_">
                    <span class="block_obj_desc_title">Описание</span>
                    %desc%
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="row-fluid">
            <div id="YMapsID" class="span8"></div>
        </div>
    </div>
</div>
<script type="text/javascript">
    %map%
</script>